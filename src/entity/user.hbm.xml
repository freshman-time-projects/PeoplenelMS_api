<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="entity.User" table="user">
    <id name="u_id" column="u_id" type="java.lang.Integer">
    <generator class="native"></generator>
    </id>
    <property name="username" column="username" type="java.lang.String"></property>
    <property name="pasword" column="pasword" type="java.lang.String"></property>
    <property name="e_mail" column="e_mail" type="java.lang.String"></property>
    </class>
    </hibernate-mapping>